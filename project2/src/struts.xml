<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
    
<struts>
    
    <package name="user" namespace="/" extends="struts-default">
        
        <action name="createUserPage" class="in.dac.UsersAction" method="createUserPage">
            <result name="success">/createUserPage.jsp</result>
        </action>
        
        <action name="createUser" class="in.dac.UsersAction" method="createUser">
            <result name="success">/listAllUser.jsp</result>
        </action>
        
        <action name="editUser" class="in.dac.UsersAction" method="editUser">
            <result name="success">/createUserPage.jsp</result>
        </action>
    </package>
   
    
</struts>    
    